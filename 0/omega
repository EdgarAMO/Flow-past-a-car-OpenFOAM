FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

#include        "../initializer"

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform $omega;

boundaryField
{
    inlet
    {
        type                freestream;
        freestreamValue     $internalField;
    }

    outlet
    {
        type                freestream;
    freestreamValue         $internalField;
    }

    top
    {
        type                omegaWallFunction;
        value               uniform 1e3;
    }

    bottom
    {
        type                omegaWallFunction;
        value               uniform 1e3;
    }

    sides
    {
        type                omegaWallFunction;
        value               uniform 1e3;
    }

    myCar
    {
        type                omegaWallFunction;
        value               uniform 1e3;
    }
}

// ************************************************************************* //
