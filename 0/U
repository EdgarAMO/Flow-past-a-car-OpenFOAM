FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

#include        "../initializer"

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform ($U 0 0);

boundaryField
{
    inlet
    {
        type            freestream;
        freestreamValue $internalField;
    }

    outlet
    {
        type            freestream;
        freestreamValue $internalField;
    }

    top
    {
        type            noSlip;
    }

    bottom
    {
        type            noSlip;
    }

    sides
    {
        type            noSlip;
    }

    myCar
    {
        type            noSlip;
    }
}

